{
    "extends": "airbnb",
    "plugins": [
        "compat"
    ],
    "rules": {
        "compat/compat": "error",
        "react/prop-types": "off",
        "jsx-a11y/anchor-is-valid": "off",
        "jsx-a11y/label-has-for": [
            "off",
            {
                "components": [
                    "label"
                ],
                "required": {
                    "some": [
                        "nesting",
                        "id"
                    ]
                }
            }
        ]
    }
}